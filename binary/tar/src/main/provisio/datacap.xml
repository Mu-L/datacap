<runtime>
    <!-- Target -->
    <archive name="datacap-${project.version}.tar.gz" hardLinkIncludes="**/*.jar" />

    <!-- Basic configure -->
    <fileSet to="/">
        <directory path="${basedir}/copy-conf/">
            <include>bin/*</include>
            <include>configure/*</include>
            <include>configure/*/*</include>
            <include>configure/*/*/*</include>
            <include>functions/*</include>
            <include>schema/*</include>
        </directory>
    </fileSet>

    <!-- Server -->
    <artifactSet to="lib">
        <artifact id="io.edurt.datacap:datacap-server:${project.version}" />
    </artifactSet>

    <!-- Plugins -->
    <artifactSet to="plugins/http-clickhouse">
        <artifact id="io.edurt.datacap.plugin.http:datacap-plugin-http-clickhouse:${project.version}">
        </artifact>
    </artifactSet>
    <artifactSet to="plugins/http-cratedb">
